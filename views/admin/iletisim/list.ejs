<aside class="right-side">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <h1>
      İletişim Listesi

    </h1>

  </section>

  <!-- Main content -->
  <section class="content">

    <div class="box">

      <div class="box-body">
        <table class="table table-bordered">
          <tbody><tr>
            <th style="width: 10px">id</th>
            <th>Kullanıcı Id</th>
            <th>Iletisim Tipi</th>
            <th>Değer</th>

            <th style="width: 40px">Düzenle</th>
            <th style="width: 40px">Sil</th>
          </tr>

          <% _.each(iletisim, function(ilet) {%>
          <tr>
            <td><%=ilet.id %></td>
            <td><%=ilet.kullaniciId%></td>
            <td><%=ilet.iletisimTipId%></td>
            <td><%=ilet.deger%></td>

            <td><a href="/admin/iletisim/edit/<%=ilet.id %>"><span class="badge bg-green">düzenle</span></a></td>
            <td><a onclick="return confirm('Silmek İstediğinizden Eminmisiniz ?');" href="/admin/iletisim/delete/<%=ilet.id %>"><span class="badge bg-red">sil</span></a></td>
          </tr>
          <% }) %>

          </tbody></table>
      </div><!-- /.box-body -->
      <div class="box-footer clearfix ">
        <div class="pagination-sm no-margin pull-right">

        </div>
      </div>
    </div>
  </section>
  <!-- /.content -->
</aside>
