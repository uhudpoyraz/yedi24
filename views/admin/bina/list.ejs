<aside class="right-side">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <h1>
      Bina Listesi

    </h1>

  </section>

  <!-- Main content -->
  <section class="content">

    <div class="box">

      <div class="box-body">
        <table class="table table-bordered">
          <tbody><tr>
            <th style="width: 10px">id</th>
            <th>Bina İsmi</th>
            <th>Bina Kodu</th>
            <th style="width: 40px">Düzenle</th>
            <th style="width: 40px">Sil</th>
          </tr>
          <% _.each(binalar, function(bina) {%>
          <tr>
            <td><%=bina.id %></td>
            <td><%=bina.isim%></td>
            <td><%=bina.kod%></td>
            <td><a href="/admin/bina/edit/<%=bina.id %>"><span class="badge bg-green">düzenle</span></a></td>
            <td><a onclick="return confirm('Silmek İstediğinizden Eminmisiniz ?');" href="/admin/bina/delete/<%=bina.id %>"><span class="badge bg-red">sil</span></a></td>
          </tr>
          <% }) %>

          </tbody></table>
      </div><!-- /.box-body -->
      <div class="box-footer clearfix ">
        <div class="pagination-sm no-margin pull-right">

        </div>
      </div>
    </div>
  </section>
  <!-- /.content -->
</aside>
