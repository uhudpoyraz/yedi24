<aside class="right-side">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <h1>
      Şikayet Durum Tipi Listesi

    </h1>

  </section>

  <!-- Main content -->
  <section class="content">

    <div class="box">

      <div class="box-body">
        <table class="table table-bordered">
          <tbody><tr>
            <th style="width: 10px">id</th>
            <th>Şikayet Durum Tipi</th>

            <th style="width: 40px">Düzenle</th>
            <th style="width: 40px">Sil</th>
          </tr>

          <% _.each(durumtipleri, function(tip) {%>
          <tr>
            <td><%=tip.id %></td>
            <td><%=tip.isim%></td>

            <td><a href="/admin/sikayetdurumtipi/edit/<%=tip.id%>"><span class="badge bg-green">düzenle</span></a></td>
            <td><a onclick="return confirm('Silmek İstediğinizden Eminmisiniz ?');" href="/admin/sikayetdurumtipi/delete/<%=tip.id %>"><span class="badge bg-red">sil</span></a></td>
          </tr>
          <% }) %>

          </tbody></table>
      </div><!-- /.box-body -->
      <div class="box-footer clearfix ">
        <div class="pagination-sm no-margin pull-right">

        </div>
      </div>
    </div>
  </section>
  <!-- /.content -->
</aside>
